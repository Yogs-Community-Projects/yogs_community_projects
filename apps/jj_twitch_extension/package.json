    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.5.0"
  },
  "dependencies": {
    "@kobalte/core": "^0.10.0",
    "@kobalte/tailwindcss": "^0.7.0",
    "@solidjs/router": "^0.8.2",
    "@stitches/core": "^1.2.8",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/luxon": "^3.2.0",
    "@types/pako": "^2.0.0",
    "@ycapp/common": "1.0.0",
    "@ycapp/commonui": "1.0.0",
